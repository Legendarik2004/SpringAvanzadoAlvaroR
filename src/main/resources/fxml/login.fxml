<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.springavanzadoalvaror.ui.pantallas.LoginController">

    <TextField fx:id="username" layoutX="255.0" layoutY="100.0" promptText="Username" prefWidth="90.0"/>
    <TextField fx:id="password" layoutX="255.0" layoutY="150.0" promptText="Password" prefWidth="90.0"/>
    <Button layoutX="270.0" layoutY="200.0" mnemonicParsing="false" onAction="#doLogin" text="Login" prefWidth="60.0"/>
    <Button layoutX="270.0" layoutY="250.0" mnemonicParsing="false" onAction="#doRegister" text="Register"/>
</AnchorPane>
